#! /bin/sh

# Test that within Armour Item::stackable is initialized to false
# print twice, 
# first, check for attempted funny business with functions--e.g., Armour::isStackable
# second, force use of Item::isStackable
# both outputs must evaluate to false

cat <<EOF > test005.cpp

#include <iostream>
#include <iomanip>

#include "Armour.h"

using std::cout;

int main(){
    Item *armour = new Armour();    

    cout.setf( std::ios::boolalpha );

    cout << armour->isStackable()       << "\n";
    cout << armour->Item::isStackable() << "\n";   

    return 0;
}
EOF

g++ -c test005.cpp 
g++ -o test005 test005.o Armour.o Item.o

./test005