#! /bin/sh

# Test that within Consumable Item::stackable is initialized to true
# print twice, 
# first, check for attempted funny business with functions--e.g., Consumable::isStackable
# second, force use of Item::isStackable
# both outputs must evaluate to true

cat <<EOF > test010.cpp

#include <iostream>
#include <iomanip>

#include "Consumable.h"

using std::cout;

int main(){
    Item *consumable = new Consumable();    

    cout.setf( std::ios::boolalpha );

    cout << consumable->isStackable()       << "\n";
    cout << consumable->Item::isStackable() << "\n";   

    return 0;
}
EOF

g++ -c test010.cpp 
g++ -o test010 test010.o Consumable.o Item.o

./test010